  <project remote="freescale" name="meta-freescale-ml" path="sources/meta-freescale-ml"/>
  <project remote="freescale" name="Documentation" path="sources/Documentation"/>
  <project remote="oss" revision="master" name="meta-browser" path="sources/meta-browser"/>
  <project remote="rust"  revision="master" name="meta-rust" path="sources/meta-rust"/>
  <project remote="clang" revision="master" name="meta-clang" path="sources/meta-clang"/>
  <project remote="playground" revision="upstream/master" name="meta-qt5" path="sources/meta-qt5"/>
  <project remote="boundary" name="meta-boundary" path="sources/meta-boundary"/>

</manifest>
